{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"title": "CampaignDetails",
	"description": "Schema for the CampaignDetails Tile created and associated to each Campaign",
	"type": "object",
	"properties": {
		"destination_url": {
			"type": "string"
		},
		"name": {
			"type": "string"
		},
		"description": {
			"type": "string"
		},
		"image": {
			"type": "string"
		},
		"external_link": {
			"type": "string"
		}
	},
	"required": ["destination_url", "name"]
}
